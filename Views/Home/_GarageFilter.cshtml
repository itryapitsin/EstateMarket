<div class="row space1" ng-show="realtyType == 'garages'">
    <div class="col-lg-12">
        <label>Вид объекта</label>
        <select style="width: 100%; height: 30px;" ng-model="objectType">
            <option value="">{{emptyObjectType}}</option>
            <option value="garage">Гараж</option>
            <option value="carplace">Машиноместо</option>
        </select>
    </div>
</div>

<div class="row space1" ng-show="objectType == 'garage'">
    <div class="col-lg-12">
        <label>Тип гаража</label>
        <select style="width: 100%; height: 30px;" ng-model="garageType">
            <option value="">{{emptyGarageType}}</option>
            <option value="ferroconcrete">Железобетонный</option>
            <option value="brick">Кирпичный</option>
            <option value="metallic">Металлический</option>
        </select>
    </div>
</div>

<div class="row space1" ng-show="objectType == 'carplace'">
    <div class="col-lg-12">
        <label>Тип машиноместа</label>
        <select style="width: 100%; height: 30px;" ng-model="garageType">
            <option value="">{{emptyGarageType}}</option>
            <option value="openParking">Открытая стоянка</option>
            <option value="parking">Крытая стоянка</option>
            <option value="undergroundParking">Подземный паркинг</option>
            <option value="multilevelParking">Многоуровневый паркинг</option>
        </select>
    </div>
</div>

<div class="row space1" ng-show="realtyType == 'garages'">
    <div class="col-lg-12">
        <label>Охрана</label>
        <select style="width: 100%; height: 30px;" ng-model="security">
            <option value="">{{emptySecurity}}</option>
            <option value="yes">Да</option>
            <option value="no">Нет</option>
        </select>
    </div>
</div>
