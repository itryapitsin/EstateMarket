<div class="row space1" ng-show="realtyType == 'commercials'">
    <div class="col-lg-12" ng-class="{ 'has-error': errors['shouldInputObjectType'] }">
        <label class="control-label">Вид объекта</label>
        <select style="width: 100%; height: 30px;" ng-model="objectType" class="form-control">
            <option value="">{{emptyObjectType}}</option>
            <option value="hotel">Гостиница</option>
            <option value="office">Офисное помещение</option>
            <option value="general">Помещение свободного назначения</option>
            <option value="production">Производственное помещение</option>
            <option value="trade">Торговое помещение</option>
            <option value="warehouse">Складское помещение</option>
        </select>
    </div>
</div>